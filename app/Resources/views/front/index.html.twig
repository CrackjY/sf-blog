{% extends 'base.html.twig' %}

{% block content%}
	{% for firstname in firstnames|sort %}
		{% if firstname|length <= 4 %}
			{{ firstname }}
		{% endif %}
	{% endfor %}
	</br></br>
	{% for people in peoples %}
		<h5>Parents : </h5>
		{{ people.firstname }}
		{{ people.lastname }} 
		{% for child in people.children %}
			<h5>Children : </h5>
			{{ child.firstname }}
			{{ child.lastname }}
			{{ child.age }}
		{% endfor %}</br></br>
	{% endfor %}
{% endblock %}